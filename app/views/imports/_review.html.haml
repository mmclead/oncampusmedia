%h3 Import complete
- if @new_schools.present?
  %h4 Newly Created Schools
  .well.auto-over
    = render partial: 'schools/full_school_record', locals: {schools: @new_schools}

%h4 Updated Schools
.well.auto-over.limit-height
  = render partial: 'changed_attrs', locals: {schools: @import.send("imported_"+file.name.to_s.slice(0..-6))[1]}