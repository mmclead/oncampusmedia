          <li> <%= link_to "Quotes", ratecards_url %> </li>
        </ul>
      </div>
    </div>
  </div>
  <div class="clearfix"></div>
  <div class="container">
    <%= yield %>
  </div>
  <%= yield :scripts %>

</body>
</html>
