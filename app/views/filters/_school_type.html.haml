.accordian-group
  .accordian-heading
    %h4= link_to 'School Type', '#school-type', { data: {toggle: "collapse", parent: "#school-info-list"}}
  #school-type.accordian-body.collapse
    #all-types
      = check_box_tag "All Types", 'all', true
      = label_tag "All Types", "All Types"
    %ul.well
      - ["4 Year", "CC"].each do |school_type|  
        %li
          = check_box_tag "#{school_type}"
          = label_tag "#{school_type}", "#{school_type}"
    
