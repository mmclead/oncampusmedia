%li.school-list-item
  = check_box_tag "throw[#{school.name.parameterize.underscore.to_sym}]", school.store_id, false, id: school.name.titleize
  <label for= 
  = "#{school.name.titleize}"
  > 
  = "#{school.name.titleize} - #{school.store_id}"
  </label>
  = link_to 'Edit', [:edit, school] if can? :update, school
%li.divider