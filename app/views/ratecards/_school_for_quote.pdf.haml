- schools.each do |school|
  %tr
    %td= school.store_id
    %td= school.name
    %td= school.school_type
    %td= school.address
    %td= school.city
    %td= school.state
    %td= school.dma
    %td= school.dma_rank
    %td= school.num_of_screens
    %td= school.demographics.enrollment
    %td= number_with_delimiter(@ratecard.total_impressions([school]).to_i)
    %td= number_to_currency(@ratecard.total_cost([school]))
    